{
    printf("Hello World\n");
    printf("OK");

    return 0;
}
#include <stdio.h>
#include <math.h>


int main()

{
    float i,a,b,c,d,x1,x2;
    i = 1;
    while (i==1.0)
    {
    scanf("%f",&a );
    scanf("%f",&b );
    scanf("%f",&c );
    d = b*b - 4*a*c;
    printf("d = %g \n", d);
    x1 = (-b + pow(d, (1/2)))/(2*a);
    x2 = (-b - pow(d, (1/2)))/(2*a);
    printf("x1 = %g \n", x1);
    printf("x2 = %g \n", x2);
    printf("Yes - 1, No- 0 \n");
    scanf("%f",&i );
    }
    return 0;
}
